public class Cast1{
public static void main(String[]args){
double d;//int to double
d=145;
System.out.println("d="+d);// small to bigger:byte>short>char>int>long>float>double

int s;//char to int
s='a';
System.out.println("s="+s);

int A;//short to int
A=-120;
System.out.println("A="+A);

double g; //flaot to double
g=105.4f;
System.out.println("g="+g);

byte b=110;
System.out.println("b="+b);

}
}

